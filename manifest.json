{
  "manifest_version": 2,

  "name": "ZEFR Pull Request Helper",
  "description": "This extension aids the user in creating a pull requeset per Nick Rotondo's PR Template",
  "version": "0.1",

  "page_action": {
    /*"default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },*/
    "default_title": "ZEFR-PRH"      // optional; shown in tooltip
    /*"default_popup": "popup.html" */
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
  {
    "matches": [
      "https://github.com/*"
    ],
    "js": ["content.js"]
  }
]
}
